  "private": "true",
  "engines": {
    "node": ">= 5.0"
  },
  "scripts": {
    "start": "npm run start-www & npm run start-worker",
    "start-www": "pm2-dev run bin/www > /dev/null",
    "start-www-debug": "iron-node bin/www",
    "start-worker": "pm2-dev run bin/worker > /dev/null",
    "start-worker-debug": "iron-node bin/worker",
    "tail": "pm2 logs --raw | bunyan -o short",
    "test": "NODE_ENV=test babel-tape-runner test/ | tap-spec",
    "sequelize": "sequelize",
    "lint": "eslint ."
  },
  "devDependencies": {
    "babel-eslint": "^5.0.0-beta6",
