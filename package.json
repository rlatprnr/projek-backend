    "iron-node": "^2.2.16",
    "nock": "^7.7.2",
    "pm2": "^1.0.0",
    "proxyquire": "^1.7.3",
    "sinon": "^1.17.3",
    "supertest": "^1.1.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.4.0"
  },
